        "prisma": "^5.22.0",
        "express-rate-limit": "^7.1.0",
        "swagger-ui-express": "^5.0.0",
        "@types/node": "^20.10.0",
        "@types/express": "^4.17.21",
        "@types/cors": "^2.8.17",
        "typescript": "^5.3.3",
